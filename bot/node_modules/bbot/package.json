    "lint": "tslint -p .",
    "local": "ts-node src/start",
    "prebuild": "yarn test",
    "prerelease": "yarn build",
    "pretest": "yarn lint",
    "release": "standard-version",
    "release:dry": "yarn release -- --dry-run",
    "release:pre": "yarn release -- --prerelease",
    "stage": "git add -A && git status -s",
    "start": "node dist/start",
    "test": "nyc mocha **/*.spec.ts",
    "test:bail": "mocha src/**/*.spec.ts --bail",
    "test:coverage": "set -o allexport; source .env; set +o allexport; codecov",
    "test:debug": "mocha --inspect --debug-brk src/**/*.spec.ts",
    "test:watch": "nodemon --exec 'yarn test:bail --silent || true'"
